{
	"name": "vvv",
	"displayName": "vvv",
	"author": "haotengfei",
	"publisher": "haotengfei",
	"icon": "img/logo.png",
	"description": "Vue3 Snippet",
	"license": "MIT",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.5.0"
	},
	"keywords": [
        "javascript",
        "typescript",
        "snippet",
        "vue",
        "vue 2",
        "vue 3"
    ],
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"repository": {
        "type": "git",
        "url": "https://github.com/TengFeiHao/vue-snippets"
    },
    "bugs": {
        "url": "https://github.com/TengFeiHao/vue-snippets/issues"
    },
	"contributes": {
		"languages": [
            {
                "id": "vue",
                "aliases": [
                    "Vue",
                    "vue"
                ],
                "extensions": [
                    ".vue"
                ]
            }
        ],
		"snippets": [
			{
				"language": "vue",
				"path": "./snippets/snippets_vue.json"
			},
			{
				"language": "typescript",
				"path": "./snippets/snippets_ts.json"
			}
		]
	},
	"scripts": {
		"publish": "vsce publish"
	}
}